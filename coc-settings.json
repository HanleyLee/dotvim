    "list.source.diagnostics.defaultAction": "",
    "list.source.diagnostics.defaultOptions": [] ,

    // wxss
    "wxss.lint.float": "ignore",
    "wxss.validate": true,
    "wxss.lint.ieHack": "ignore",
    "wxss.lint.boxModel": "ignore",
    "wxss.lint.zeroUnits": "ignore",
    "wxss.lint.important": "ignore",
    "wxss.lint.emptyRules": "warning",
    "wxss.lint.idSelector": "ignore",
    "wxss.lint.vendorPrefix": "warning",
    "wxss.lint.hexColorLength": "error",
    "wxss.lint.importStatement": "ignore",
    "wxss.colorDecorators.enable": true,
    "wxss.lint.universalSelector": "ignore",
    "wxss.lint.unknownProperties": "warning",
    "wxss.lint.fontFaceProperties": "warning",
    "wxss.lint.duplicateProperties": "ignore",
    "wxss.lint.compatibleVendorPrefixes": "ignore",
    "wxss.lint.argumentsInColorFunction": "error",
    "wxss.lint.propertyIgnoredDueToDisplay": "warning",
    "wxss.lint.unknownVendorSpecificProperties": "ignore",

    "languageserver": {
        "bash": {
            "command": "bash-language-server",
            "args": ["start"],
            "filetypes": ["sh", "zsh"],
            "ignoredRootPaths": ["~"]
        },
        "lua": {
            "command": "lua-lsp",
            "filetypes": [
                "lua"
            ]
        },
        "efm": {
            "command": "efm-langserver",
            "args": [],
            "filetypes": [
                "eruby"
                /* "vim", */
                /* "markdown" */
            ]
        },
        "sql": {
            "command": "sql-language-server",
            "args": [
                "up",
                "--method",
                "stdio"
            ],
            "filetypes": [
                "sql",
                "mysql"
            ]
        },
        "haskell": {
            "command": "haskell-language-server-wrapper",
            "args": [
                "--lsp"
            ],
            "rootPatterns": [
                "*.cabal",
                "stack.yaml",
                "cabal.project",
                "package.yaml",
                "hie.yaml"
            ],
            "filetypes": [
                "haskell",
                "lhaskell"
            ]
        },
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": [
                "dockerfile"
            ],
            "args": [
                "--stdio"
            ]
        },
        "clojure-lsp": {
            "command": "bash",
            "args": [
                "-c",
                "clojure-lsp"
            ],
            "filetypes": [
                "clojure"
            ],
            "rootPatterns": [
                "project.clj"
            ],
            "additionalSchemes": [
                "jar",
                "zipfile"
            ],
            "trace.server": "verbose",
            "initializationOptions": {}
        }
    }
}
